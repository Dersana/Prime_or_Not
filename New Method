#include<stdio.h>

#include<conio.h>



void main()

{

 int i,j,n,x;

 clrscr();



 printf("\n Enter any Number to check whether it is prime or not \n");

 scanf("%d",&n);



 for ( i= 1 ; i<x ; i++)

 {

  for ( j = 0; j <=x ; j++)

  {

    y = i*j;

    if(x==y && i>1)

    {

     printf( " Number is Not Prime \n");

     getch();

     exit(0);

    }

  }

 }

 printf("\n Number is Prime " );

 getch();

}



